# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ACCESS_TOKEN=your-supabase-access-token

# J-Quants API
JQUANTS_API_KEY=your-jquants-api-key

# Cron Security
CRON_SECRET=your-cron-secret

# Resend (Email Notification)
RESEND_API_KEY=your-resend-api-key
ALERT_EMAIL_TO=your-email@example.com

# Sentry (Error Monitoring)
SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxxxx
NEXT_PUBLIC_SENTRY_DSN=https://xxx@xxx.ingest.sentry.io/xxxxx
SENTRY_ORG=your-org-slug
SENTRY_PROJECT=your-project-name
SENTRY_AUTH_TOKEN=your-auth-token

# e-Stat API
ESTAT_API_KEY=your-estat-api-key

# Optional
SYNC_MAX_CATCHUP_DAYS=5
INVESTOR_TYPES_WINDOW_DAYS=60
